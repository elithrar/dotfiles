# OrbStack SSH (conditional - only included if OrbStack is installed)
Include ~/.orbstack/ssh/config

Host *
  ServerAliveInterval 60
  ForwardAgent yes
  ControlMaster auto
  ControlPath /tmp/ssh_mux_%h_%p_%r
  UseKeychain yes

Host rs
  Hostname 45.87.250.130
  User user
  Port 2222

Host github.com
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/id_ed25519
