{
  "icon_theme": "Zed (Default)",
  "edit_predictions": {
    "mode": "eager"
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "show_whitespaces": "boundary",
  "vim_mode": true,
  "ui_font_family": "Berkeley Mono",
  "buffer_font_family": "Berkeley Mono",
  "ui_font_size": 15,
  "buffer_font_size": 14,
  "ui_font_weight": 500,
  "soft_wrap": "editor_width",
  "theme": {
    "mode": "dark",
    "light": "Catppuccin Mocha",
    "dark": "Catppuccin Mocha"
  },
  "formatter": "auto",
  "languages": {
    "Markdown-Inline": {
      "enable_language_server": true
    }
  },
  "git_panel": {
    "dock": "right"
  },
  "project_panel": {
    "dock": "right"
  },
  "outline_panel": {
    "dock": "right"
  },
  "cursor_shape": "block",
  "agent": {
    "inline_assistant_model": {
      "provider": "anthropic",
      "model": "claude-3-5-sonnet-latest"
    },
    "profiles": {
      "write": {
        "name": "Write",
        "tools": {
          "terminal": true,
          "batch_tool": true,
          "code_symbols": true,
          "copy_path": true,
          "create_file": true,
          "delete_path": true,
          "diagnostics": true,
          "edit_file": true,
          "edit_files": false,
          "fetch": true,
          "list_directory": true,
          "move_path": true,
          "now": true,
          "find_path": true,
          "read_file": true,
          "grep": true,
          "symbol_info": true,
          "thinking": true
        },
        "enable_all_context_servers": true,
        "context_servers": {}
      }
    },
    "always_allow_tool_actions": true,
    "dock": "right",
    "enabled": true,
    "default_model": {
      "provider": "anthropic",
      "model": "claude-3-7-sonnet-thinking-latest"
    }
  },
  "terminal": {
    "font_family": "Berkeley Mono",
    "blinking": "off",
    "working_directory": "current_project_directory"
  },
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  }
}
