{
  "agent_buffer_font_size": 15.0,
  "icon_theme": "Zed (Default)",
  "edit_predictions": {
    "mode": "eager",
    "disabled_globs": [
      "**/.env*",
      "**/*.pem",
      "**/*.key"
    ]
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "show_whitespaces": "boundary",
  "vim_mode": true,
  "ui_font_family": "Berkeley Mono",
  "buffer_font_family": "Berkeley Mono",
  "ui_font_size": 15,
  "buffer_font_size": 14,
  "ui_font_weight": 500,
  "soft_wrap": "editor_width",
  "relative_line_numbers": "enabled",
  "current_line_highlight": "gutter",
  "inlay_hints": {
    "enabled": true
  },
  "vim": {
    "use_system_clipboard": "always"
  },
  "theme": "Cloudflare Dark",
  "languages": {
    "Markdown-Inline": {
      "enable_language_server": true
    },
    "Markdown": {
      "soft_wrap": "editor_width",
      "format_on_save": "off"
    }
  },
  "git_panel": {
    "dock": "right"
  },
  "project_panel": {
    "dock": "right"
  },
  "outline_panel": {
    "dock": "right"
  },
  "notification_panel": {
    "dock": "right"
  },
  "collaboration_panel": {
    "dock": "right"
  },
  "cursor_shape": "block",
  "scrollbar": {
    "show": "auto",
    "git_diff": true,
    "diagnostics": "all"
  },
  "agent": {
    "inline_assistant_model": {
      "provider": "anthropic",
      "model": "claude-sonnet-4-5-latest"
    },
    "profiles": {
      "write": {
        "name": "Write",
        "tools": {
          "terminal": true,
          "batch_tool": true,
          "code_symbols": true,
          "copy_path": true,
          "create_file": true,
          "delete_path": true,
          "diagnostics": true,
          "edit_file": true,
          "edit_files": false,
          "fetch": true,
          "list_directory": true,
          "move_path": true,
          "now": true,
          "find_path": true,
          "read_file": true,
          "grep": true,
          "symbol_info": true,
          "thinking": true
        },
        "enable_all_context_servers": true,
        "context_servers": {}
      }
    },
    "always_allow_tool_actions": true,
    "dock": "right",
    "enabled": true,
    "default_model": {
      "provider": "anthropic",
      "model": "claude-sonnet-4-5-latest"
    }
  },
  "terminal": {
    "font_family": "Berkeley Mono",
    "blinking": "off",
    "working_directory": "current_project_directory"
  },
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  }
}
