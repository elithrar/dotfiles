{
  "icon_theme": "Zed (Default)",
  "edit_predictions": {
    "mode": "eager_preview"
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "show_whitespaces": "boundary",
  "vim_mode": true,
  "ui_font_family": "Berkeley Mono",
  "buffer_font_family": "Berkeley Mono",
  "ui_font_size": 15,
  "buffer_font_size": 14,
  "ui_font_weight": 500,
  "soft_wrap": "editor_width",
  "theme": {
    "mode": "dark",
    "light": "Night Owl Light",
    "dark": "Catppuccin Mocha"
  },
  "formatter": "auto",
  "languages": {
    "Markdown-Inline": {
      "enable_language_server": true
    }
  },
  "git_panel": {
    "dock": "right"
  },
  "project_panel": {
    "dock": "right"
  },
  "outline_panel": {
    "dock": "right"
  },
  "cursor_shape": "block",
  "assistant": {
    "enable_experimental_live_diffs": true,
    "dock": "right",
    "enabled": true,
    "version": "2",
    "default_model": {
      "provider": "anthropic",
      "model": "claude-3-7-sonnet-latest"
    }
  },
  "terminal": {
    "font_family": "Berkeley Mono",
    "blinking": "off",
    "working_directory": "current_project_directory"
  },
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  }
}
